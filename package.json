{
  "name": "ioserver",
  "version": "1.2.9",
  "description": "Damn simple way to setup your Socket.io server using coffeescript or vanilla JS class.",
  "main": "build/ioserver.js",
  "files": [
    "build/"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "fastify": "^3.20.1",
    "fastify-cors": "^6.0.2",
    "fastify-sensible": "^3.1.1",
    "fastify-socket.io": "^2.0.0",
    "socket.io": "^3.1.2"
  },
  "devDependencies": {
    "chai": "^4.3.3",
    "chai-http": "^4.3.0",
    "coffeescript": "^2.5.1",
    "mocha": "^9.1.0",
    "snyk": "^1.467.0",
    "socket.io-client": "^3.1.2"
  },
  "scripts": {
    "dev": "$(npm bin)/coffee --no-header --map --watch --output build/ --compile src/",
    "build": "$(npm bin)/coffee --no-header --map --output build/ --compile src/",
    "test": "$(npm bin)/coffee --no-header -c ./test && mocha --exit",
    "snyk-protect": "$(npm bin)/snyk protect",
    "prepublish": "npm run snyk-protect",
    "gpr-setup": "node scripts/gpr.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/x42en/IOServer.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "socket.io",
    "coffeescript",
    "webservice",
    "network",
    "x42en",
    "websockets"
  ],
  "author": "Ben Mz",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/x42en/IOServer/issues"
  },
  "homepage": "https://github.com/x42en/IOServer#readme",
  "optionalDependencies": {},
  "snyk": true
}
